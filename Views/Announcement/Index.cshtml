
@{
    ViewData["Title"] = "Index";
}

<div class="main-panel-div3">
    <div class="main-panel-inner">
        <b class="announcements-b4">Announcements</b>
        <div class="search-buttons">
            <input class="search-announcements-input"
                   type="search"
                   placeholder="Office staff or teachers..."
                   autofocus />
            <div class="dropdown-time">
                <div class="helper-button-icon">
                    <div class="helper-button-label">This Week...</div>
                    <img class="chevron-down-icon"
                         alt=""
                         src="public/chevrondown.svg" />
                </div>
            </div>
        </div>
        <div class="container-make-announcement">
            <button class="button-add-staff-member1"
                    autofocus
                    id="buttonAddStaffMember">
                <b class="button-label2">Make Announcement</b>
            </button>
        </div>
        <div class="dashboard-div3" id="Table - View Office Staff">
            <div class="column-div4">
                <div class="header-div14"><div class="title-div">Title</div></div>
                <div class="content-div44">
                    <div class="text-group-basic21">
                        <b class="final-exam-b1">Final Exam</b>
                    </div>
                </div>
                <div class="divider-solid44">
                    <div class="baseline-div44"></div>
                </div>
            </div>
            <div class="column-div5">
                <div class="header-div15">
                    <div class="title-div">Details</div>
                </div>
                <div class="content-div45">
                    <div class="the-final-exam-will-start-on-a">
                        The Final Exam will start on April 23. The Schedule will be
                        posted on April 2.
                    </div>
                </div>
                <div class="divider-solid44">
                    <div class="baseline-div44"></div>
                </div>
            </div>
            <div class="column-div6">
                <div class="header-div16">
                    <div class="title-div">Actions</div>
                </div>
                <div class="content-div46">
                    <img class="chevron-down-icon"
                         alt=""
                         src="public/pencilalt7.svg" />
                    <div class="title-div">|</div>
                    <img class="chevron-down-icon" alt="" src="public/trash8.svg" />
                </div>
                <div class="divider-solid44">
                    <div class="baseline-div44"></div>
                </div>
                <div class="divider-solid44">
                    <div class="baseline-div47"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="right-panel-div2">
    <div class="notifications-div1" id="Table - Notifications">
        <div class="header-notifications1">
            <img class="icon-bell1" alt="" src="public/icon--bell.svg" /><b class="notifications-b1">Notifications</b>
        </div>
        <div class="divider-solid48"><div class="baseline-div48"></div></div>
        <div class="notifications-row-16">
            <div class="text-group-basic22">
                <div class="final-exam-b1">Final Exam is on Feb 26</div>
                <div class="weeks-left-div2">2 weeks left</div>
            </div>
            <div class="frame-div3"><div class="mins-div2">05 mins</div></div>
        </div>
        <div class="divider-solid49"><div class="baseline-div49"></div></div>
        <div class="notifications-row-16">
            <div class="text-group-basic22">
                <div class="final-exam-b1">Sports day will be held on Mar 16</div>
                <div class="weeks-left-div2">3 weeks left</div>
            </div>
            <div class="frame-div3"><div class="mins-div2">25 mins</div></div>
        </div>
        <div class="divider-solid49"><div class="baseline-div49"></div></div>
        <div class="notifications-row-16">
            <div class="text-group-basic22">
                <div class="final-exam-b1">Watch found on the 1st floor.</div>
                <div class="weeks-left-div2">
                    Go to the lost and found on the...
                </div>
            </div>
            <div class="frame-div3"><div class="mins-div2">2 hrs</div></div>
        </div>
    </div>
    <div class="view-complaints-div2" id="Table - Appointments">
        <div class="header-notifications1">
            <div class="icon-complaints1">
                <img class="vector-complaints1"
                     alt=""
                     src="public/vector--complaints.svg" />
            </div>
            <b class="notifications-b1">Appointments</b>
        </div>
        <div class="divider-solid48"><div class="baseline-div48"></div></div>
        <div class="appointments-row-12">
            <div class="text-group-basic25">
                <div class="the-final-exam-will-start-on-a">
                    Mr. Levi Ackermann
                </div>
            </div>
            <div class="apr-11-at-130pm1">Apr 11 at 1:30pm</div>
            <div class="days-left-div3">2 days left</div>
        </div>
        <div class="divider-solid49"><div class="baseline-div49"></div></div>
        <div class="appointments-row-12">
            <div class="text-group-basic25">
                <div class="the-final-exam-will-start-on-a">
                    Mrs. Kuchel Ackermann
                </div>
            </div>
            <div class="apr-11-at-130pm1">Apr 13 at 1:30pm</div>
            <div class="days-left-div3">4 days left</div>
        </div>
        <div class="divider-solid49"><div class="baseline-div49"></div></div>
        <div class="appointments-row-12" id="Appointment Row #3">
            <div class="text-group-basic25">
                <div class="the-final-exam-will-start-on-a">Dr. Eren Jager</div>
            </div>
            <div class="apr-11-at-130pm1">Apr 18 at 1:30pm</div>
            <div class="days-left-div3">9 days left</div>
        </div>
        <div class="divider-solid49"><div class="baseline-div49"></div></div>
    </div>
</div>

